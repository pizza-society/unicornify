  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^6.3.0",
    "@vuelidate/core": "^2.0.0",
    "@vuelidate/validators": "^2.0.0",
    "axios": "^1.3.2",
    "bootstrap": "^5.2.3",
    "install": "^0.13.0",
    "pinia": "^2.0.30",
    "v-calendar": "^2.4.1",
    "vue": "^3.2.47",
    "vue-router": "^4.1.6",
    "vuex": "^4.1.0",
    "yarn": "^1.22.19"
  },
  "devDependencies": {
    "@types/jest": "^29.4.0",
    "@typescript-eslint/eslint-plugin": "^5.51.0",
    "@typescript-eslint/parser": "^5.51.0",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vue/eslint-config-typescript": "^11.0.2",
    "@vue/test-utils": "^2.2.10",
    "@vue/tsconfig": "^0.1.3",
    "@vue/vue3-jest": "^29.2.2",
    "eslint": "^8.34.0",
    "eslint-plugin-vue": "^9.9.0",
    "jest": "^29.4.2",
    "sass": "^1.58.0",
    "ts-jest": "^29.0.5",
    "typescript": "^4.9.5",
    "vite": "^4.1.1",
    "vitest": "^0.28.4",
    "vue-tsc": "^1.0.24"
  }
